{
  "name": "simple boost for kanbanpad.com",
  "version": "0.6.3",
  "update_url": "https://raw.github.com/onjin/chrome-kanbanpad-boost/master/updates.xml",
  "description": "Add ability to hide/unhide columns",
  "icons": { "16": "icon16.png",
             "48": "icon48.png",
            "128": "icon128.png" },
  "content_scripts": [
    {
        "matches": ["https://www.kanbanpad.com/*" ],
        "css": ["kanbanpad_boost.css"],
        "js": ["jquery-1.7.1.min.js", "jquery.cookie.js", "jquery.qtip-1.0.0-rc3.min.js", "kanbanpad_boost.js"]
    }
  ],
  "permissions": [
    "https://www.kanbanpad.com/"
  ]
}
